<% unless @registrations.empty? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5>Registrations:</h5>
    <ul class="side-nav">
      <% @registrations.each do |camp| %>
        <li><%= link_to camp.name, camp_path(camp) %></li>
        <li><%= "#{camp.start_date.strftime("%m/%d/%y")}" %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>Student has not attended any camps in the past.</h5>
  </div>
<% end %>