<% unless @students.empty? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5>Students:</h5>
    <ul class="side-nav">
      <% @students.each do |student| %>
        <li><%= link_to "#{student.first_name} #{student.last_name}", student_path(student) %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>No students listed in this family.</h5>
  </div>
<% end %>